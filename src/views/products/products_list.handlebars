<table>
  <thead>
    <tr>
      <th>ID</th>
      <th>Producto</th>
      <th>Proveedor</th>
      <th>Departamento</th>
      <th>Precio</th>
    </tr>
  </thead>
  <tbody>
    {{#each products}}
      <tr>
        <td>{{this._id}}</td>
        <td>{{this.nombre}}</td>
        <td>{{this.proveedor}}</td>
        <td>{{this.keyDepartment}}</td>
        <td>{{this.price}}</td>
      </tr>
    {{/each}}
  </tbody>
</table>